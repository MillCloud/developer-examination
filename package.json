{
  "name": "developer-examination",
  "private": true,
  "version": "1.2.1",
  "scripts": {
    "build": "tsc && vite build --base=/developer-examination/",
    "commit": "cz",
    "dev": "vite",
    "lint": "yarn run lint:json && yarn run lint:markdown && yarn run lint:script && yarn run lint:style",
    "lint:json": "prettier ./**/*.json --write --ignore-path=.gitignore",
    "lint:markdown": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:script": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:style": "stylelint ./**/*.{css,less,scss,vue} --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "serve": "vite preview"
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@ant-design/icons": "~4.6.3",
    "@iconify/react": "~3.0.1",
    "@windicss/animations": "~1.0.8",
    "@windicss/plugin-heropatterns": "~0.0.8",
    "@windicss/plugin-interaction-variants": "~1.0.0",
    "@windicss/plugin-question-mark": "~0.1.1",
    "@windicss/plugin-scrollbar": "~1.2.3",
    "antd": "~4.16.12",
    "file-saver": "~2.0.5",
    "markdown-to-jsx": "~7.1.3",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-markdown": "~7.0.0",
    "react-router": "~5.2.0",
    "react-router-config": "~5.1.1",
    "react-router-dom": "~5.2.0",
    "react-syntax-highlighter": "~15.4.4",
    "windicss": "~3.1.7"
  },
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/eslint-parser": "~7.15.0",
    "@commitlint/cli": "~13.1.0",
    "@modyqyw/fabric": "~3.0.2",
    "@types/file-saver": "~2.0.3",
    "@types/node": "~16.6.1",
    "@types/react": "~17.0.18",
    "@types/react-dom": "~17.0.9",
    "@types/react-router-config": "~5.0.3",
    "@types/react-router-dom": "~5.1.8",
    "@types/react-syntax-highlighter": "~13.5.2",
    "@typescript-eslint/eslint-plugin": "~4.29.2",
    "@typescript-eslint/parser": "~4.29.2",
    "@vitejs/plugin-react-refresh": "~1.3.6",
    "@vitejs/plugin-vue": "~1.4.0",
    "@vue/compiler-sfc": "~3.2.4",
    "eslint": "~7.32.0",
    "husky": "~7.0.1",
    "is-ci": "~3.0.0",
    "less": "~4.1.1",
    "lint-staged": "~11.1.2",
    "markdownlint-cli": "~0.28.1",
    "prettier": "~2.3.2",
    "release-it": "~14.11.5",
    "stylelint": "~13.13.1",
    "typescript": "~4.3.5",
    "vite": "~2.5.0",
    "vite-plugin-pages": "~0.17.3",
    "vite-plugin-style-import": "~1.1.1",
    "vite-plugin-windicss": "~1.2.7",
    "vite-react-jsx": "~1.1.2",
    "vite-tsconfig-paths": "~3.3.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
