{
  "name": "developer-examination",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --base=/developer-examination/",
    "serve": "vite preview",
    "lint:json": "prettier ./**/*.json --write --ignore-path=.gitignore",
    "lint:script": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:style": "stylelint ./**/*.{css,less,sass,scss,vue} --fix --allow-empty-input --ignore-path=.gitignore",
    "lint:markdown": "markdownlint . --fix --ignore-path=.gitignore",
    "commit": "cz",
    "prepare": "is-ci || husky install",
    "lint": "yarn run lint:json && yarn run lint:markdown && yarn run lint:script && yarn run lint:style",
    "test": "yarn run lint",
    "release": "release-it"
  },
  "dependencies": {
    "@ant-design/icons": "~4.6.2",
    "@windicss/animations": "~1.0.8",
    "@windicss/plugin-icons": "~1.0.2",
    "@windicss/plugin-question-mark": "~0.1.1",
    "@windicss/plugin-scrollbar": "~1.2.3",
    "antd": "~4.16.6",
    "file-saver": "~2.0.5",
    "markdown-to-jsx": "~7.1.3",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-router": "~5.2.0",
    "react-router-config": "~5.1.1",
    "react-router-dom": "~5.2.0",
    "ress": "~4.0.0",
    "windicss": "~3.1.4"
  },
  "devDependencies": {
    "@babel/core": "~7.14.6",
    "@babel/eslint-parser": "~7.14.7",
    "@commitlint/cli": "~12.1.4",
    "@modyqyw/fabric": "~2.5.2",
    "@types/file-saver": "~2.0.2",
    "@types/node": "~16.0.0",
    "@types/react": "~17.0.13",
    "@types/react-dom": "~17.0.8",
    "@types/react-router-config": "~5.0.2",
    "@types/react-router-dom": "~5.1.7",
    "@typescript-eslint/eslint-plugin": "~4.28.1",
    "@typescript-eslint/parser": "~4.28.1",
    "@vitejs/plugin-react-refresh": "~1.3.4",
    "@vitejs/plugin-vue": "~1.2.4",
    "@vue/compiler-sfc": "~3.1.4",
    "eslint": "~7.30.0",
    "husky": "~7.0.0",
    "is-ci": "~3.0.0",
    "less": "~4.1.1",
    "lint-staged": "~11.0.0",
    "markdownlint-cli": "~0.27.1",
    "prettier": "~2.3.2",
    "release-it": "~14.10.0",
    "stylelint": "~13.13.1",
    "typescript": "~4.3.5",
    "vite": "~2.3.8",
    "vite-plugin-pages": "~0.14.8",
    "vite-plugin-style-import": "~1.0.1",
    "vite-plugin-windicss": "~1.2.0",
    "vite-react-jsx": "~1.1.1",
    "vite-tsconfig-paths": "~3.3.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
