{
  "name": "developer-examination",
  "version": "1.8.6",
  "private": true,
  "author": "MillCloud",
  "scripts": {
    "build": "vite build --base=/developer-examination/",
    "check:deps": "ncu",
    "check:types": "vue-tsc",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint && vue-tsc --noEmit",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue,svelte}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "serve": "vite preview -- port 5050"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "~0.2.6",
    "@iconify/vue": "~3.1.3",
    "@modyqyw/utils": "~2.1.2",
    "@vueuse/core": "~7.5.5",
    "@vueuse/integrations": "~7.5.5",
    "axios": "~0.25.0",
    "browser-update": "~3.3.37",
    "color": "~4.2.0",
    "element-plus": "~1.3.0-beta.10",
    "element-pro-components": "~0.17.1",
    "file-saver": "~2.0.5",
    "highlight.js": "~11.4.0",
    "modern-normalize": "~1.1.0",
    "vue": "~3.2.29",
    "vue-query": "~1.19.0",
    "vue3-markdown-it": "~1.0.10"
  },
  "devDependencies": {
    "@babel/core": "~7.17.0",
    "@babel/eslint-parser": "~7.17.0",
    "@commitlint/cli": "~16.1.0",
    "@iconify/json": "~2.0.33",
    "@modyqyw/fabric": "~4.11.2",
    "@tailwindcss/aspect-ratio": "~0.4.0",
    "@tailwindcss/forms": "~0.4.0",
    "@tailwindcss/line-clamp": "~0.3.1",
    "@tailwindcss/typography": "~0.5.1",
    "@types/browser-update": "~3.3.0",
    "@types/color": "~3.0.3",
    "@types/file-saver": "~2.0.5",
    "@typescript-eslint/eslint-plugin": "~5.10.2",
    "@typescript-eslint/parser": "~5.10.2",
    "@vitejs/plugin-vue": "~2.1.0",
    "@vitejs/plugin-vue-jsx": "~1.3.3",
    "@vue/tsconfig": "~0.1.3",
    "commitizen": "~4.2.4",
    "cross-env": "~7.0.3",
    "eslint": "~8.8.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.3.3",
    "markdownlint-cli": "~0.31.0",
    "npm-check-updates": "~12.5.0",
    "picocolors": "~1.0.0",
    "postcss": "~8.4.6",
    "postcss-flexbugs-fixes": "~5.0.2",
    "postcss-preset-env": "~7.3.1",
    "prettier": "~2.5.1",
    "release-it": "~14.12.4",
    "rimraf": "~3.0.2",
    "sass": "~1.49.7",
    "shelljs": "~0.8.5",
    "stylelint": "~14.3.0",
    "tailwindcss": "~3.0.18",
    "type-fest": "~2.11.1",
    "typescript": "~4.5.5",
    "unplugin-icons": "~0.13.0",
    "unplugin-vue-components": "~0.17.17",
    "vite": "~2.7.13",
    "vite-plugin-compression": "~0.5.1",
    "vite-plugin-env-compatible": "~1.1.1",
    "vite-plugin-eslint": "~1.3.0",
    "vite-plugin-mkcert": "~1.5.2",
    "vite-plugin-stylelint": "~1.1.0",
    "vue-tsc": "~0.31.2"
  }
}
